{
  "name": "aleksei4er/task-news-parser",
  "description": "A task news parser",
  "license": "MIT",
  "keywords": [
    "task",
    "news",
    "parser"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Aleksei4er\\TaskNewsParser\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aleksei4er\\TaskNewsParser\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aleksei4er\\TaskNewsParser\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
